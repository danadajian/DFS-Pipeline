{
  "name": "dfs-pipeline",
  "version": "1.0.0",
  "scripts": {
    "build": "tsc",
    "test": "jest --runInBand"
  },
  "dependencies": {
    "@types/aws-sdk": "^2.7.0",
    "@types/axios": "^0.14.0",
    "@types/cheerio": "^0.22.17",
    "@types/dotenv": "^8.2.0",
    "@types/jest": "^25.2.1",
    "@types/lodash": "^4.14.149",
    "@types/node": "^12.0.0",
    "@types/xml2js": "^0.4.5",
    "aws-sdk": "latest",
    "axios": "^0.19.2",
    "cheerio": "latest",
    "dotenv": "^8.2.0",
    "jest": "^25.3.0",
    "lodash": "^4.17.15",
    "ts-jest": "^25.4.0",
    "typescript": "^3.7.5",
    "xml2js": "^0.4.23"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/build/",
      "<rootDir>/node_modules/"
    ]
  }
}
